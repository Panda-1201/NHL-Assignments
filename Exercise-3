import csv
import numpy as np
import matplotlib.pyplot as plt
from typing import Set, Tuple, List

    with open(csv_file_path, 'r') as result_csv:
        csv_reader = csv.reader(result_csv, delimiter=',')
        next(csv_reader)
        for row in csv_reader:
            results.append(row)

    # Convert list of string rows to a float NumPy array
    results = np.stack(results).astype(float)

    # Plot the data: results[:, 0] is Precision, results[:, 1] is Recall
    plt.plot(results[:, 0], results[:, 1])
    plt.ylim([-0.05, 1.05])
    plt.xlim([-0.05, 1.05])

    plt.xlabel('Precision')
    plt.ylabel('Recall')

    plt.show()

plt.xlabel('Precision')
plt.ylabel('Recall')

plt.show()
